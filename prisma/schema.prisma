datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String  @id @default(uuid())
  email    String  @unique
  password String
  oauthTokens OAuthToken[]
}

model OAuthToken{
  token   String  @id 
  expiresAt DateTime 
  userId  String  
  user   User @relation(fields: [userId], references: [id])
}